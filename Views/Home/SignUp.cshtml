@{
    ViewData["Title"] = "Sign Up";
}

<div class="text-center main-login-container">
    <form class="column-display form-width"action='@Url.Action("SignUp", "Account")' method="POST"  autocomplete="off">
        <h1>Registrate</h1>
        <label>Elegí un usuario</label>
        <input type="text" id="user" name="user" placeholder="StartDust_03">
        <label>Ingresa tu email</label>
        <input type="email" id="mail" name="mail" placeholder="start@mail.com">
        <label>Creá tu contraseña</label>
        <input type="text" id="passw" name="passw" placeholder="*******">
        <input type="submit" value="Registrarme">
    </form>
    <div class="row-display">    
        <a href='@Url.Action("Index")'>Ya tengo cuenta</a>
    </div>
</div>